@model IEnumerable<MarketResearchManagment.Models.MarketMap>

<h2>Market Maps</h2>

@foreach (var map in Model)
{
    <div>
        <h3>@map.Name</h3>
        <p>@map.Description</p>

        <!-- Assuming you have images stored in a folder named 'MarketMaps' in your 'Content' directory -->
        <img src="@Url.Content(map.ImageUrl)" alt="Map Image" style="width:100%;max-width:600px;" />
    </div>
}

